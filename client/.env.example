# Example Client Environment Variables - Copy to .env and fill with actual values

# API URL - Your backend server URL
REACT_APP_API_URL=http://localhost:5001/api

# Firebase Client Configuration
# Get these from Firebase Console → Project Settings → General → Your apps → Web app config
REACT_APP_FIREBASE_API_KEY=your-firebase-api-key
REACT_APP_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your-firebase-project-id
REACT_APP_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your-sender-id
REACT_APP_FIREBASE_APP_ID=1:123456789:web:abcdef123456

# Cloudinary Configuration (for image uploads)
REACT_APP_CLOUDINARY_CLOUD_NAME=ddalwamja
REACT_APP_CLOUDINARY_UPLOAD_PRESET=products
